-- SETTINGS
local function Slider(text, min, max, callback)
	local b = Instance.new("TextButton", SetPage)
	b.Size = UDim2.new(1,0,0,40)
	b.Text = text
	b.BackgroundColor3 = Color3.fromRGB(35,35,35)
	b.TextColor3 = Color3.new(1,1,1)
	Instance.new("UICorner", b)
	b.MouseButton1Click:Connect(function()
		callback(math.random(min,max))
	end)
end

Slider("‡∏™‡∏∏‡πà‡∏°‡∏™‡∏µ GUI",0,255,function()
	UIStroke.Color = Color3.fromRGB(math.random(0,255),0,255)
end)

Slider("‡∏õ‡∏£‡∏±‡∏ö‡∏Ç‡∏ô‡∏≤‡∏î GUI",300,700,function(v)
	Main.Size = UDim2.new(0,v,0,v/1.4)
end)

-- Tabs logic
TabBtn1.MouseButton1Click:Connect(function()
	FuncPage.Visible = true
	SetPage.Visible = false
end)

TabBtn2.MouseButton1Click:Connect(function()
	FuncPage.Visible = false
	SetPage.Visible = true
end)

-- Close/Open
Top.InputBegan:Connect(function(i)
	if i.KeyCode == Enum.KeyCode.RightShift then
		Main.Visible = not Main.Visible
		OpenBtn.Visible = not Main.Visible
	end
end)

OpenBtn.MouseButton1Click:Connect(function()
	Main.Visible = true
	OpenBtn.Visible = false
end)
